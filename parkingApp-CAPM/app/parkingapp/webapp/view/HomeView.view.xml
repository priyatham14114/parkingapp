                            <m:Label
                                id="idLab33r__le1"
                                text="Vendor Name"
                            />
                            <m:Input
                                id="idVendorName___"
                                placeholder="Enter Vendor Name"
                                change=""
                            />
                        </m:VBox>
                        <m:VBox
                            id="idvBox7"
                            class="Inputs"
                        >
                            <m:Label
                                id="_IDGenLabel555"
                                text="Delivery Type"
                            />
                            <m:Select id="idTypeOfDelivery">
                                <core:Item
                                    id="_IDGenItem234"
                                    key="InBound"
                                    text="InBound"
                                />
                                <core:Item
                                    id="_IDGenItem2344"
                                    key="OutBound"
                                    text="OutBound"
                                />
                            </m:Select>
                        </m:VBox>
                        <m:VBox
                            id="idvBox4"
                            class="Inputs"
                        >
                            <m:Label 
                                id="_IDGenLabel1"
                                text="Parking Slot"
                            />
                            <m:Select
                                id="parkingLotSelect"
                                items="{path: '/parkingSlots', filters: [{path: 'status', operator: 'EQ', value1: 'Available'}]}"
                            >
                                <core:Item
                                    id="_IDGenItem3"
                                    key="{ID}"
                                    text="{slotNumbers}"
                                />
                            </m:Select>
                        </m:VBox>
                        <!-- <m:VBox id="idvBox5">
                            <m:Label
                                id="idLable5"
                                text="Check In"
                            />
                            <m:Input id="input5" />
                        </m:VBox> -->
                        <m:VBox
                            id="idvBox6"
                            class="Inputs"
                        >
                            <m:Label id="idLable6" />
                            <m:Button
                                id="idButtonAssign"
                                type="Default"
                                text="Assign"
                                press="onAssignPress"
                            />
