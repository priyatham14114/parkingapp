        title="Confirm Rejection"
        class="dialogSlideIn"
        resizable="true"
        draggable="true"
        contentWidth="35%"
        contentHeight="15%"
    >
        <content>
            <form:SimpleForm
                id="_I23enSimpleForm1"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="3"
                labelSpanL="3"
                labelSpanM="3"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
